{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SearchEngines": {
    "Google": {
      "URL": "https://www.google.com/search?q={0}&start={1}",
      "MatchingExpression": "(<div class=\"BNeawe UPmit AP7Wnd\">)(\\w+[a-zA-Z0-9.-?=/]*)"
    },
    "Bing": {
      "URL": "https://www.bing.com/search?q={0}&first={1}",
      "MatchingExpression": "(<li class=\"b_algo\"><h2><a href=\")(\\w+[a-zA-Z0-9.-?=/]*)"
    }
  }
}
